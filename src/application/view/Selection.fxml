<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.SelectionController">
   <children>
      <AnchorPane fx:id="app" prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: #333952;">
         <children>
            <ImageView fitHeight="200.0" fitWidth="301.0" layoutX="303.0" layoutY="370.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../image/Toilet-2.png" />
               </image>
            </ImageView>
            <Button fx:id="hover" layoutX="113.0" layoutY="207.0" mnemonicParsing="false" text="Button" />
         </children>
      </AnchorPane>
      <ComboBox fx:id="buildingDropDown" layoutX="75.0" layoutY="57.0" onAction="#pickBuilding" prefWidth="170.0" promptText="BUILDING" />
      <Button layoutX="658.0" layoutY="7.0" mnemonicParsing="false" onAction="#gotoLogin" prefHeight="46.0" prefWidth="124.0" style="-fx-background-color: Transparent;" text="LOGOUT" textFill="#fff8f8">
         <font>
            <Font name="American Typewriter Bold" size="14.0" />
         </font></Button>
      <ComboBox fx:id="restroomDropDown" layoutX="325.0" layoutY="57.0" onAction="#pickRestroom" prefWidth="150.0" promptText="RESTROOM" />
      <Button layoutX="545.0" layoutY="57.0" mnemonicParsing="false" onAction="#gotoReviews" prefHeight="27.0" prefWidth="61.0" text="GO!" />
   </children>
</AnchorPane>
